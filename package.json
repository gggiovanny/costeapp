{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "run-p prepare:*",
    "prepare:husky": "husky install",
    "prepare:dev-env": "cp .env.example .env",
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:arc": "cross-env NODE_ENV=development arc sandbox",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production arc sandbox",
    "lint": "eslint --max-warnings 0 --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "typecheck": "tsc -b",
    "prettier": "npx prettier --write --config .prettierrc .",
    "pre-commit": "run-s prettier typecheck lint"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.2.5",
    "@mantine/hooks": "^5.2.5",
    "@mantine/notifications": "^5.2.5",
    "@mantine/remix": "^5.2.5",
    "@remix-run/architect": "^1.6.8",
    "@remix-run/node": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@architect/architect": "^10.3.4",
    "@remix-run/dev": "^1.6.8",
    "@remix-run/eslint-config": "^1.7.0",
    "@types/eslint": "^8.4.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  }
}
