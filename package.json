{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "run-p prepare:*",
    "prepare:husky": "husky install",
    "prepare:prisma": "prisma generate",
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:arc": "cross-env NODE_ENV=development arc sandbox",
    "dev": "remix dev",
    "start": "remix-serve build",
    "lint": "eslint --max-warnings 0 --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "typecheck": "tsc -b",
    "prettier": "npx prettier --write --config .prettierrc .",
    "pre-commit": "run-s prettier typecheck lint",
    "db:migrate": "prisma migrate dev",
    "db:prototype": "prisma db push",
    "seed": "node --require esbuild-register prisma/seed.ts",
    "db:view": "npx prisma studio"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mantine/core": "^5.2.5",
    "@mantine/dates": "^5.5.4",
    "@mantine/hooks": "^5.2.5",
    "@mantine/modals": "^5.3.3",
    "@mantine/notifications": "^5.2.5",
    "@mantine/remix": "^5.2.5",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@remix-validated-form/with-zod": "^2.0.1",
    "@types/lodash": "^4.14.184",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "framer-motion": "^7.3.5",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "remix-validated-form": "^4.5.5",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.19.0"
  },
  "devDependencies": {
    "@prisma/client": "^4.3.1",
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@types/eslint": "^8.4.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  }
